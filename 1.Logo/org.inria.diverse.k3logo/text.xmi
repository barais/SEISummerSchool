<?xml version="1.0" encoding="ASCII"?>
<logoDSL:LogoProgram xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:logoDSL="http://www.inria.fr/diverse/logo/DSL">
  <sentences xsi:type="logoDSL:CLEAR"/>
  <sentences xsi:type="logoDSL:MAKE" name="color">
    <value xsi:type="logoDSL:VALUE" val="10.0"/>
  </sentences>
  <sentences xsi:type="logoDSL:REPEAT">
    <commands xsi:type="logoDSL:LEFT">
      <amount xsi:type="logoDSL:VALUE" val="90.0"/>
    </commands>
    <commands xsi:type="logoDSL:PENDOWN"/>
    <commands xsi:type="logoDSL:FORWARD">
      <amount xsi:type="logoDSL:VALUE" val="50.0"/>
    </commands>
    <times xsi:type="logoDSL:VALUE" val="4.0"/>
  </sentences>
</logoDSL:LogoProgram>
